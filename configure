#!/bin/bash

OUT=./bin
BUILD=$OUT/build

echo $BUILDSUB

if [ ! -d "$OUT" ]; then
	mkdir $OUT
fi
if [ ! -d "$BUILD" ]; then
	mkdir $BUILD
    for fold in $(find ./src -type d | sed 's/^\.\/src\///' | tail -n+2); do
        if [ ! -d "$BUILD/$fold" ]; then        
            mkdir $BUILD/$fold
        fi
    done
fi
